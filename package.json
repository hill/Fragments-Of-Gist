  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/user/repo.git"
  },
  "author": "Tom Hill",
  "scripts": {
    "test": "xo",
    "start": "electron .",
    "build": "electron-packager . --out=dist --asar --overwrite --all",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds-new",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds-new --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Fragments of Gist\""
  },
  "dependencies": {
    "electron-debug": "^1.0.0",
    "howler": "^2.0.3",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "mousetrap": "^1.6.1",
    "rellax": "^1.0.0",
    "scrollmagic": "^2.0.5"
  },
  "devDependencies": {
    "devtron": "^1.1.0",
    "electron": "^1.6.10",
    "electron-packager": "^8.7.2",
    "xo": "^0.18.0"
  },
  "xo": {
    "envs": [
      "node",
      "browser"
    ]
  },
  "bugs": {
    "url": "https://github.com/user/repo/issues"
  },
  "homepage": "https://github.com/user/repo#readme",
